<窗口
	宽度="600"
	高度="600"
	显示确认按钮="true"
	显示标题栏="true"
	标题="-----"
	标签头高度="-2"
	配置文件="main.config"
	标签头背景="#ffffffff"
	标签头字体大小="16"
	标签头选中字体颜色="#ffff3377"
	标签头字体默认颜色="#ff3355ff">
	<标签页
		标题="设置"
		背景="#ffffffff">
		<水平布局
        	id="idLayoutHor1"
        	宽度="-1"
        	高度="-1"
        	背景="#002222222">
            <文本框 id="idLabel2" 默认值="战斗模式设置" 文字大小="0" 高度="-1" 宽度="-2" 边距="0,0,0,0"/>
            <下拉框
            	id="战斗模式设置"
            	宽度="-1"
            	边距="0,0,0,0"
            	默认值="0">
            	<选项 文本="L1"/>
            </下拉框>
        </水平布局>
        <水平布局
        	id="idLayoutHor2"
        	宽度="-1"
        	高度="-1"
        	背景="#002222222">
            <文本框 id="idLabel2" 默认值="--------------战斗设置(L1)--------------" 文字大小="0" 高度="-1" 宽度="-2" 边距="0,0,0,0"/>
        </水平布局>
        <水平布局
        	id="idLayoutHor1"
        	宽度="-1"
        	高度="-1"
        	背景="#002222222">
            <文本框 id="idLabel3" 默认值="下牌方向:" 文字大小="0" 高度="-1" 宽度="-2" 边距="0,0,0,0"/>
            <下拉框
            	id="下牌方向_L1"
            	宽度="-1"
            	边距="0,0,0,0"
            	默认值="0">
            	<选项 文本="左路"/>
            	<选项 文本="右路"/>
            	<选项 文本="随机"/>
            </下拉框>
            <文本框 id="idLabel4" 默认值="下牌位置:" 文字大小="0" 高度="-1" 宽度="-2" 边距="0,0,0,0"/>
            <下拉框
            	id="下牌位置_L1"
            	宽度="-1"
            	边距="0,0,0,0"
            	默认值="0">
            	<选项 文本="国王塔沉底"/>
            	<选项 文本="公主塔后沉底"/>
            	<选项 文本="中置"/>
                <选项 文本="桥头"/>
            </下拉框>
            <文本框 id="idLabel5" 默认值="下牌时机:" 文字大小="0" 高度="-1" 宽度="-2" 边距="0,0,0,0"/>
            <下拉框
            	id="下牌时机_L1"
            	宽度="-1"
            	边距="0,0,0,0"
            	默认值="2">
            	<选项 文本="10圣水"/>
            	<选项 文本="9圣水"/>
            	<选项 文本="8圣水"/>
                <选项 文本="7圣水"/>
                <选项 文本="6圣水"/>
                <选项 文本="5圣水"/>
                <选项 文本="4圣水"/>
                <选项 文本="3圣水"/>
                <选项 文本="2圣水"/>
                <选项 文本="1圣水"/>
            </下拉框>
        </水平布局>
        <水平布局
            id="idLayoutHor3"
            宽度="-1"
            高度="-1"
            背景="#002222222">
            <文本框 id="idLabel2" 默认值="------------------------------------------------" 文字大小="0" 高度="-1" 宽度="-2" 边距="0,0,0,0"/>
        </水平布局>
        
	</标签页>
    
</窗口>